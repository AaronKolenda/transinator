<div class="newStationWrap">
	<h1>Add a new station to your dashboard</h1>

	<div class="newStationsSearch">
		<label>Longitude: <input type="text" id="longInput"/></label>
		<label>Latitude: <input type="text" id="latInput"/></label>
		<a href="#"><button id="search">Search</button></a>
	</div>

	<div id="resultsWrap">

	<div class="row center">

		<div class="cell columns3">Metro Stations</div>

		<div class="cell columns3">Bus Stations</div>

		<div class="cell columns3">Bike Stations</div>

	</div>


	<div class="cell columns3">
		<div class="row">
			<div class="cell columns3" id="rails">
				<script type="x-handlebars-template" id="rails-template">
					{{each}}
					<div class="row">
						<div class="infoWrap cell  test">
							{{name}} informationinformation information
						</div> 
					</div>

					<div class="row">
						<div class="infoWrap cell  test">
							{{somedata}} information information information
						</div> 
					</div>

					<div class="row">
						<div class="infoWrapn cell  test">
							{{somedata}} information information information
						</div>
					</div> 

					<div class="row">
						<div class="infoWrap cell  test">
							{{somedata}}information information information
						</div> 
					</div>
					{{/each}}
				</script>	
			</div>
		</div>
	</div>

